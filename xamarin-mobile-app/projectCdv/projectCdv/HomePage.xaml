<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="projectCdv.HomePage"
             xmlns:local='clr-namespace:projectCdv'>

    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Add" Command="{Binding NavCommand}" />
    </TabbedPage.ToolbarItems>

    <local:HistoryPage Title="History" />
    <local:MapPage Title="Places" />
    <local:ProfilePage Title="Profile" />
    <local:TestPage Title="Test" />

</TabbedPage>