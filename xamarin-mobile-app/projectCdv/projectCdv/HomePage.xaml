<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="projectCdv.HomePage"
             xmlns:local='clr-namespace:projectCdv'>

    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Add" Icon="ic_action_add.png" Priority="0" Order="Primary" Command="{Binding NavCommand}" />
    </TabbedPage.ToolbarItems>

    <local:HistoryPage Title="History" IconImageSource="ic_action_home.png" />
    <local:MapPage Title="Places" IconImageSource="ic_action_place.png" />
    <local:ProfilePage Title="Profile" IconImageSource="ic_action_person.png" />
    <local:TestPage Title="Test" IconImageSource="ic_action_settings_input_hdmi.png" />

</TabbedPage>